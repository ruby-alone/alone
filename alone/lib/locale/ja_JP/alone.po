#: ../al_form.rb:359
msgctxt "al"
msgid "Size is too large."
msgstr "サイズが大きすぎます。"

#: ../al_form.rb:789 ../al_form.rb:1416 ../al_form.rb:1461 ../al_form.rb:1538
msgctxt "al"
msgid "%s is required."
msgstr "%sを入力してください。"

#: ../al_form.rb:796
msgctxt "al"
msgid "%1$s must be entered within %2$d characters."
msgstr "%1$sは、最大%2$d文字で入力してください。"

#: ../al_form.rb:800
msgctxt "al"
msgid "%1$s must be at least %2$d characters long."
msgstr "%1$sは、最低%2$d文字入力してください。"

#: ../al_form.rb:805 ../al_form.rb:1469 ../al_form.rb:1548
msgctxt "al"
msgid "Please enter %s correctly."
msgstr "%sを正しく入力してください。"

#: ../al_form.rb:976 ../al_form.rb:1103
msgctxt "al"
msgid "Please select %s."
msgstr "%sを選んでください。"

#: ../al_form.rb:983 ../al_form.rb:1109
msgctxt "al"
msgid "The input for %s is not a valid value."
msgstr "%sの入力が、規定値以外です。"

#: ../al_form.rb:1094
msgctxt "al"
msgid "Please select a maximum of %2$d for %1$s."
msgstr "%1$sは、最大%2$d個選んでください。"

#: ../al_form.rb:1098
msgctxt "al"
msgid "Please select a minimum of %2$d for %1$s."
msgstr "%1$sは、最低%2$d個選んでください。"

#: ../al_form.rb:1424
msgctxt "al"
msgid "Please enter %s as an integer."
msgstr "%sは整数で入力してください。"

#: ../al_form.rb:1430
msgctxt "al"
msgid "%1$s must be %2$d or less."
msgstr "%1$sは、%2$d以下を入力してください。"

#: ../al_form.rb:1434
msgctxt "al"
msgid "%1$s must be %2$d or more."
msgstr "%1$sは、%2$d以上を入力してください。"

#: ../al_form.rb:1475
msgctxt "al"
msgid "%1$s must be %2$f or less."
msgstr "%1$sは、%2$f以下を入力してください。"

#: ../al_form.rb:1479
msgctxt "al"
msgid "%1$s must be %2$f or more."
msgstr "%1$sは、%2$f以上を入力してください。"

#: ../al_form.rb:1553
msgctxt "al"
msgid "%1$s must be no more than %2$s."
msgstr "%1$sは、最大%2$sまでで入力してください。"

#: ../al_form.rb:1557
msgctxt "al"
msgid "%1$s must be at least %2$s."
msgstr "%1$sは、最小%2$sまでで入力してください。"

#: ../al_form/input_file.rb:52
msgctxt "al"
msgid "Please specify %s."
msgstr "%sを指定してください。"

#: ../al_login_main.rb:97
msgctxt "al"
msgid "Invalid User ID or password."
msgstr "ユーザIDまたはパスワードが違います。"
